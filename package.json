{
  "name": "@epiclabs/epic-video-player",
  "version": "0.0.3",
  "description": "Video player wrapper to support different video sources with unified interface",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "_clean": "rimraf dist",
    "_transpile": "tsc",
    "_bundle": "webpack",
    "_npm_ready": "cp-cli CHANGELOG.md ./dist/CHANGELOG.md && cp-cli LICENSE ./dist/LICENSE && cp-cli package.json ./dist/package.json && cp-cli README.md ./dist/README.md && cp-cli demo.html ./dist/demo.html",
    "build": "run-s _clean _bundle _transpile _npm_ready",
    "test": "jest --coverage --env=jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epiclabs-io/epic-video-player.git"
  },
  "keywords": [
    "epic",
    "labs",
    "epiclabs",
    "video",
    "player",
    "dash",
    "dashjs",
    "hls",
    "hls.js"
  ],
  "author": "Adrian Caballero <https://github.com/adripanico>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/epiclabs-io/epic-video-player/issues"
  },
  "homepage": "https://github.com/epiclabs-io/epic-video-player#readme",
  "dependencies": {
    "dashjs": "2.9.2",
    "hls.js": "0.12.2",
    "stream": "0.0.2"
  },
  "devDependencies": {
    "@types/hls.js": "0.12.1",
    "@types/jest": "^23.3.13",
    "@types/node": "10.12.18",
    "cp-cli": "1.1.2",
    "jest": "23.6.0",
    "npm-run-all": "4.1.5",
    "rimraf": "2.6.3",
    "ts-jest": "^23.10.5",
    "ts-loader": "5.3.3",
    "tslib": "1.9.3",
    "typescript": "3.2.4",
    "uglifyjs-webpack-plugin": "2.1.1",
    "webpack": "4.29.0",
    "webpack-cli": "3.2.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
